generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model History {
  id        Int      @id @default(autoincrement())
  symbol    String
  date      DateTime
  value     Decimal  @db.Decimal(20, 6)
  createdAt DateTime @default(now())

  @@unique([symbol, date])
  @@index([symbol])
}


